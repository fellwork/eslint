{
  "name": "@fellwork/eslint-config-typescript",
  "version": "0.0.1",
  "description": "Typescript configurations for ESLint",
  "keywords": [
    "eslint",
    "config",
    "typescript"
  ],
  "homepage": "https://github.com/fellwork/eslint#readme",
  "bugs": {
    "url": "https://github.com/fellwork/eslint/issues",
    "email": "Fellwork <shane@fellwork.com>"
  },
  "license": "MIT",
  "author": "Fellwork <shane@fellwork.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellwork/eslint.git",
    "directory": "packages/typescript"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "*.md"
  ],
  "scripts": {
    "build": "unbuild",
    "docs": "typedoc",
    "preinstall": "npx only-allow pnpm",
    "watch": "unbuild --stub"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "typescript": ">=3.9"
  },
  "dependencies": {
    "@fellwork/eslint-config-core": "workspace:*",
    "@fellwork/eslint-shared": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint-plugin-import": "^2.26.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "typescript": "^4.6.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "unbuild": {
    "clean": true,
    "declaration": true,
    "entries": [
      "src/index"
    ],
    "outDir": "lib",
    "rollup": {
      "emitCJS": true
    }
  }
}
