{
  "name": "@fellwork/eslint-config-core",
  "version": "0.0.1",
  "description": "Core ESLint rules and standards",
  "keywords": [
    "eslint",
    "config",
    "core",
    "builtin",
    "builtins",
    "built-in",
    "rules"
  ],
  "homepage": "https://github.com/fellwork/eslint#readme",
  "bugs": {
    "url": "https://github.com/fellwork/eslint/issues",
    "email": "Fellwork <shane@fellwork.com>"
  },
  "license": "MIT",
  "author": "Fellwork <shane@fellwork.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellwork/eslint.git",
    "directory": "packages/core"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "*.md"
  ],
  "scripts": {
    "build": "unbuild",
    "docs": "typedoc",
    "preinstall": "npx only-allow pnpm",
    "watch": "unbuild --stub"
  },
  "peerDependencies": {
    "eslint": ">=8.8.0"
  },
  "dependencies": {
    "@fellwork/eslint-config-env": "workspace:*",
    "@fellwork/eslint-config-import": "workspace:*",
    "@fellwork/eslint-shared": "workspace:*",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-yml": "^0.14.0",
    "jsonc-eslint-parser": "^2.1.0",
    "yaml-eslint-parser": "^0.5.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "unbuild": {
    "clean": true,
    "declaration": true,
    "entries": [
      "src/index"
    ],
    "outDir": "lib",
    "rollup": {
      "emitCJS": true
    }
  }
}
