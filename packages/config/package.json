{
  "name": "@fellwork/eslint-config",
  "version": "0.1.0",
  "description": "Shared ESLint config",
  "keywords": [
    "eslint",
    "config",
    "shared"
  ],
  "homepage": "https://github.com/fellwork/eslint#readme",
  "bugs": {
    "url": "https://github.com/fellwork/eslint/issues",
    "email": "Fellwork <shane@fellwork.com>"
  },
  "license": "MIT",
  "author": "Fellwork <shane@fellwork.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellwork/eslint.git",
    "directory": "packages/config"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "*.md"
  ],
  "scripts": {
    "build": "unbuild",
    "docs": "typedoc",
    "preinstall": "npx only-allow pnpm",
    "release": "standard-version",
    "watch": "unbuild --stub"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8",
    "typescript": "^4"
  },
  "dependencies": {
    "@fellwork/eslint-config-react": "workspace:*",
    "@fellwork/eslint-config-vue": "workspace:*"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "unbuild": {
    "clean": true,
    "declaration": true,
    "entries": [
      "src/index"
    ],
    "outDir": "lib",
    "rollup": {
      "emitCJS": true
    }
  }
}
