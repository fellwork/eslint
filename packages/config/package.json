{
  "name": "@fellwork/eslint-config",
  "description": "Shared ESLint config",
  "keywords": [
    "eslint",
    "config",
    "shared"
  ],
  "homepage": "https://github.com/fellwork/eslint#readme",
  "bugs": {
    "url": "https://github.com/fellwork/eslint/issues",
    "email": "Fellwork <shane@fellwork.com>"
  },
  "license": "MIT",
  "author": "Fellwork <shane@fellwork.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellwork/eslint.git",
    "directory": "packages/config"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "*.md"
  ],
  "scripts": {
    "build": "unbuild",
    "preinstall": "npx only-allow pnpm",
    "watch": "unbuild --stub"
  },
  "peerDependencies": {
    "eslint": "^7 || ^8",
    "prettier": "^2",
    "typescript": "^4"
  },
  "dependencies": {
    "@fellwork/eslint-config-core": "workspace:*",
    "@fellwork/eslint-config-import": "workspace:*",
    "@fellwork/eslint-config-prettier": "workspace:*",
    "@fellwork/eslint-config-typescript": "workspace:*"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.13.0",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
