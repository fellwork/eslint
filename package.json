{
  "name": "@fellwork/eslint",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.0.0-rc.3",
  "description": "Fellwork ESLint monorepo",
  "keywords": [
    "eslint",
    "config",
    "format",
    "style"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fellwork/eslint.git"
  },
  "scripts": {
    "build": "turbo run build",
    "commit": "git add -a && git commit -m \"chore(release): :rocket: release \"",
    "lint:md": "prettier -c \"**/*.md\"",
    "lint:script": "eslint .",
    "format": "pnpm format:md && pnpm format:script",
    "format:md": "prettier -w \"**/*.md\"",
    "format:script": "eslint . --fix --cache",
    "prepare": "pnpm husky install",
    "push": "git push --follow-tags origin main",
    "release": "release-it"
  },
  "bugs": {
    "url": "https://github.com/fellwork/eslint/issues"
  },
  "homepage": "https://github.com/fellwork/eslint#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@fellwork/eslint-config": "*",
    "@release-it/conventional-changelog": "^4.3.0",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^17.0.24",
    "@types/prettier": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "commitlint": "^16.2.3",
    "devmoji": "^2.3.0",
    "esbuild": "^0.14.36",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "release-it": "^14.14.2",
    "release-it-lerna-changelog": "^4.0.1",
    "turbo": "^1.2.4",
    "typescript": "^4.6.3",
    "vitest": "^0.9.3"
  },
  "engines": {
    "pnpm": ">=7.0.0-rc.3"
  },
  "workspaces": [
    "packages/*"
  ]
}
