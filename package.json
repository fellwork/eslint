{
  "name": "@fellwork/eslint",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.0.1",
  "description": "Fellwork ESLint monorepo",
  "keywords": [
    "eslint",
    "config",
    "format",
    "style"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fellwork/eslint.git"
  },
  "scripts": {
    "build": "turbo run build",
    "lint": "eslint .",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "bugs": {
    "url": "https://github.com/fellwork/eslint/issues"
  },
  "homepage": "https://github.com/fellwork/eslint#readme",
  "devDependencies": {
    "@fellwork/eslint-config": "workspace:*",
    "@rushstack/eslint-patch": "latest",
    "@types/eslint": "latest",
    "@types/node": "latest",
    "bumpp": "latest",
    "esbuild": "^0.14.38",
    "eslint": "latest",
    "turbo": "latest",
    "typescript": "^4.6.4",
    "unbuild": "latest"
  },
  "engines": {
    "pnpm": ">=7.0.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
