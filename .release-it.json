{
  "git": {
    "commitMessage": "chore(release): publish ${tagName}",
    "tagName": "@fellwork/eslint@${version}"
  },
  "github": {
    "release": true
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular"
    },
    "release-it-lerna-changelog": {
      "infile": "CHANGELOG.md"
    }
  }
}
